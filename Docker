docker

 docker ps -----------------------------------------------------------------------------  docker runing contaner dekhne ke liye
 docker ps -a --------------------------------------------------------------------------  all docker contaner dekhne ke liye
 docker images -------------------------------------------------------------------------  images dekhne ke liye
  
 docker build -t nginx:web1 .  ---------------------------------------------------------  image bnane ke liye
 . ka matlb   --------------------------------------------------------------------------  dockerfile ka path 
 
 docker run -it -d --name vikas -p 1234:80 nginx:web1 ----------------------------------  contaner bnane ke liye
 -d ------------------------------------------------------------------------------------ contaner ko background me run karne ke liye 
 --name vikas --------------------------------------------------------------------------  contaner ka name 
 -p 1234:80 ----------------------------------------------------------------------------  port number 
 nginx:web1  ---------------------------------------------------------------------------  image name:tag
 
 docker-compose	
